pipeline {
  agent any
  stages { 
        stage('deploy eureka'){
          steps{
            dir('eurekaserver'){
                sh 'mvn clean install'
                sh 'cp target/eureka.war /deploy'
            }
          }
        }
        stage('deploy gateway') {
            steps{
              dir('GatewayService'){
                sh 'mvn clean install'
                sh 'cp target/gateway.war /deploy'
              }
            }
        }
    }
}
